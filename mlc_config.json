{
  "replacementPatterns": [],
  "ignorePatterns": [
    {
      "pattern": "bithesis.pdf"
    },
    {
      "pattern": "请参考|非官方的含义"
    },
    {
      "pattern": "(grd|jwc|jwb|cs).bit.edu.cn"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200,
    206
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com/",
        "https://guides.github.com/",
        "https://help.github.com/",
        "https://docs.github.com/"
      ],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ]
}
